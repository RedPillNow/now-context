{
  "name": "now-context",
  "version": "1.0.0",
  "description": "Manage the context of an application. All XHR requests should pass through this element",
  "private": true,
  "author": "Keith Strickland <keith@redpillnow.com>",
  "scripts": {
    "start": "npm run demo",
    "compile": "npm run clean-js && node -e 'require(\"./tools/compileTs\").compile(\"tsconfig.json\")'",
    "build": "npm run compile && node ./tools/build.js",
    "bump:major": "node -e 'require(\"./tools/bump-version\").bump(\"major\")'",
    "bump:minor": "node -e 'require(\"./tools/bump-version\").bump(\"minor\")'",
    "bump:patch": "node -e 'require(\"./tools/bump-version\").bump(\"patch\")'",
    "demo": "node tools/server.js demo",
    "tests": "npm run compile && node tools/server.js test",
    "clean-js": "node -e \"require('./tools/cleanCompilerFiles')\""
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.0.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.10.1",
    "@types/sinon": "^5.0.2",
    "@types/sinon-chai": "^3.2.0",
    "browser-sync": "^2.24.7",
    "chalk": "^2.4.1",
    "compress": "^0.99.0",
    "compression": "^1.7.3",
    "connect-history-api-fallback": "^1.5.0",
    "proxy-middleware": "^0.15.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@polymer/decorators": "^3.0.0",
    "@polymer/paper-item": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.1.3"
  }
}
